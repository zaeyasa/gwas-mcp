# GWAS MCP Server Environment Variables

# Logging level (DEBUG, INFO, WARNING, ERROR)
LOG_LEVEL=INFO

# Cache settings
CACHE_DIR=./cache
CACHE_ENABLED=true

# API Rate Limits (requests per minute)
ENSEMBL_RATE_LIMIT=15
GWAS_CATALOG_RATE_LIMIT=10
GTEX_RATE_LIMIT=10

# Reference genome (hg19 or hg38)
DEFAULT_GENOME_BUILD=hg38

# PLINK binary path (optional, leave empty to use pandas-plink only)
PLINK_PATH=

# Output directory for generated plots
OUTPUT_DIR=./output
